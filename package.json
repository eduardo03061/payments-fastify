{
  "name": "paymenst-ms",
  "version": "0.0.1",
  "description": "Payments microservice",
  "main": "./dist/index.js",
  "scripts": {
    "prebuild": "npm run build:clean",
    "build": "npm run tsc:build",
    "build:clean": "rm -f tsconfig.build.tsbuildinfo && rm -Rf ./dist/* ; exit 0",
    "start": "node ./dist/index.js",
    "dev": "tsnd --project tsconfig.json --files --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register -- ./src/index.ts",
    "start:debug": "tsnd --project tsconfig.json --files --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register --inspect=127.0.0.1:40100 -- ./src/index.ts",
    "lint": "eslint ./src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest --runInBand --detectOpenHandles",
    "tsc:build": "tsc -p tsconfig.build.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "package:update": "ncu -u && npm install --legacy-peer-deps && npm audit fix --legacy-peer-deps",
    "package:clean": "rm -Rf ./node_modules && rm -f ./package-lock.json && rm -f ./yarn.lock && npm run package:update"
  },
  "keywords": [],
  "author": "Ozon <info@ozon.mobi> (https://www.ozon.mobi/)",
  "contributors": [
    {
      "name": "Eduardo Hernandez",
      "email": "eduardo@ozon.mobi"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@typegoose/typegoose": "^9.4.0",
    "axios": "^0.24.0",
    "express-mongo-sanitize": "^2.1.0",
    "fastify": "^3.25.3",
    "fastify-auth": "^1.1.0",
    "fastify-autoload": "^3.9.0",
    "fastify-compress": "^4.0.1",
    "fastify-cookie": "^5.5.0",
    "fastify-cors": "^6.0.2",
    "fastify-env": "^2.1.1",
    "fastify-formbody": "^5.2.0",
    "fastify-helmet": "^5.3.2",
    "fastify-jwt": "^4.1.3",
    "fastify-multipart": "^5.2.1",
    "fastify-passport": "^0.4.3",
    "fastify-rate-limit": "^5.7.0",
    "fastify-sensible": "^3.1.2",
    "fastify-session": "^5.2.1",
    "http-status": "^1.5.0",
    "joi": "^17.5.0",
    "joi-to-json": "^2.1.1",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^12.1.5",
    "luxon": "^2.3.0",
    "mongoose": "^6.1.4",
    "mongoose-autopopulate": "^0.16.0",
    "mongoose-findorcreate": "^3.0.0",
    "mongoose-lean-virtuals": "^0.9.0",
    "mongoose-slug-updater": "^3.3.0",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pino": "^7.6.2",
    "pino-pretty": "^7.3.0",
    "qs": "^6.10.2",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/busboy": "^0.3.1",
    "@types/faker": "^5.5.9",
    "@types/http-errors": "^1.8.1",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/luxon": "^2.0.8",
    "@types/mongoose-lean-virtuals": "^0.5.2",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "faker": "^5.5.3",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "mongodb-memory-server": "^8.1.0",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "tscpaths": "^0.0.9",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.4"
  },
  "private": true,
  "engines": {
    "node": ">=16.0.0"
  },
  "optionalDependencies": {
